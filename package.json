{
  "name": "strava-steemit",
  "version": "0.1.0",
  "description": "Steemit posts from strava activities",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r351574nc3/strava-steemit.git"
  },
  "keywords": [
    "strava",
    "steemit"
  ],
  "author": "Leo Przybylski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r351574nc3/strava-steemit/issues"
  },
  "homepage": "https://github.com/r351574nc3/strava-steemit#readme",
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@nestjs/common": "^5.7.3",
    "@nestjs/core": "^5.7.3",
    "@nestjs/microservices": "^5.7.3",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/websockets": "^5.7.3",
    "@types/jsonwebtoken": "^8.3.2",
    "aws-sdk": "^2.422.0",
    "bluebird": "^3.5.4",
    "dotenv": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "nest-schedule": "^0.4.5",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "request-promise": "^4.2.4",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0",
    "steem": "^0.7.4",
    "strava": "0.0.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.4.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jest": "^29.3.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.10.1",
    "tslint": "5.12.1",
    "typescript": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
